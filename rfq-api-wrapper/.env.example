# 0x API Key
ZERO_EX_API_KEY=your_0x_api_key

# Private key for signing RFQ orders (without 0x prefix)
PRIVATE_KEY=your_private_key

# RPC URL for the chain you are providing liquidity on (e.g. Base, Ethereum)
RPC_URL=your_rpc_url

# Port for the express server
PORT=3000

# Market Maker Address (the address associated with PRIVATE_KEY)
MM_ADDRESS=your_mm_address

# Spread in Basis Points (100 bps = 1%)
# This is your profit margin. 0x price - spread = User price.
SPREAD_BPS=50
